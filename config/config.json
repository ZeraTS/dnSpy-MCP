{
  "daemon": {
    "port": 9001,
    "host": "0.0.0.0",
    "worker_pool_size": 5,
    "request_timeout_seconds": 120,
    "max_binary_size_mb": 1024,
    "api_key": null,
    "health_public": false,
    "allowed_binary_dirs": []
  },
  "dnspy": {
    "path": "/opt/dnspy/dnSpy.exe",
    "cli_path": "/opt/dnspy-mcp/cli-debugger/bin/Release/net8.0/dnspy-mcp.dll",
    "timeout_seconds": 60
  },
  "logging": {
    "level": "INFO",
    "format": "json"
  },
  "features": {
    "enable_caching": true,
    "enable_rate_limiting": true,
    "enable_metrics": true,
    "enable_structured_logging": true,
    "enable_webhooks": false
  }
}
